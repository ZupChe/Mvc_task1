<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>@ViewBag.Title</title>

    <link href="~/lib/bootstrap/dist/css/bootstrap.css" rel="stylesheet" />
    <link href="~/lib/bootstrap/dist/css/bootstrap-theme.css" rel="stylesheet" />

    <script src="~/lib/jquery/dist/jquery.js"></script>
    <script src="~/lib/jquery-validation/dist/jquery.validate.js"></script>
    <script src="~/lib/jquery-validation-unobtrusive/jquery.validate.unobtrusive.js"></script>

</head>

<body>

    @Html.Partial("/Views/_LoginPartial.cshtml")


    <header class="page-header">
        <h1 class="text-primary text-center">Dobrodošli na internet stranicu za oglašavanje automobila</h1>
    </header>

    <div class="container">
        <div class="row">
            <div class="col-md-8">
                @RenderBody()
            </div>
            <div class="col-md-4">
                <div class="panel panel-default">
                    <div class="panel-heading">
                        Brzi linkovi:
                    </div>
                    <div class="panel-body">
                        <ul>
                            <li><a href="~/">Početna</a></li>
                            <li>@Html.ActionLink("O nama", "onama", "home")</li>
                            <li>@Html.ActionLink("Kontakt", "kontakt", "home")</li>
                            <li>@Html.ActionLink("Dodaj oglas", "addoglas", "home")</li>
                            @RenderSection("SideBar", false)
                        </ul>
                    </div>

                </div>
                
            </div>

        </div>

    </div>

   

    <footer class="row">
        <p class="text-center">All rights reserved. <a href="http://www.autooglasi.eu/">Auto Oglasi</a> 2021</p>
    </footer>
</body>
</html>
@RenderSection("Scripts", false)

